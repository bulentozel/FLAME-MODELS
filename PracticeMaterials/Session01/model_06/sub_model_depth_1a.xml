<xmodel version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation='http://flame.ac.uk/schema/xmml_v2.xsd'>

<!-- Model descriptions  -->    
<name>model 06</name>
<version>1</version>
<description>Model that includes sub models</description>

<models> <!-- List of models to include with this model -->
	<model>
		<file>sub_model_depth_2.xml</file><enabled>true</enabled>
	</model>
</models>

<environment>
    <functionFiles> <!-- List of C files that hold agent functions -->
        <file>agent_a_functions_1a.c</file>
    </functionFiles>
</environment>

<!-- List of agents -->
<agents>

    <xagent>
        <name>agent_a</name>
        <description>Agent of type a</description>
        <memory></memory>
        <!-- List of agent functions -->
        <functions>          				
            <function>
                <name>from_sub_model_depth_1a</name>
                <description>A function defined in from_sub_model_depth_1a.xml</description>
                <currentState>01</currentState>
                <nextState>02</nextState>		
            </function>
        </functions>
    </xagent>

</agents>

</xmodel>